web1:
  name: wp-web
  php_extensions:
    - curl
  shared_writable_dirs:
    - wp-content/uploads/
  after_build:
    - "php pagoda/wp-install.php"
    - "cp pagoda/wp-config.php wp-config.php"
    - "rm pagoda/wp-config_a.php"
    - "rm pagoda/wp-config_b.php"
    - "rm pagoda/wp-config_c.php"
db1:
  name: wp-db
